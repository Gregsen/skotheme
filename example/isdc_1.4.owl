    </Class>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#Gravimeter -->

    <Class rdf:about="&isdc;Gravimeter">
        <rdfs:label xml:lang="en">Gravimeter</rdfs:label>
        <rdfs:comment xml:lang="en">A gravimeter is an instrument used in for measuring the gravitational field..</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&isdc;Instrument"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&isdc;isCarriedBy"/>
                <someValuesFrom rdf:resource="&isdc;SuperconductingGravimeterStation"/>
            </Restriction>
        </rdfs:subClassOf>
        <disjointWith rdf:resource="&isdc;MeteorologicalInstrument"/>
    </Class>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#ISDC -->

    <Class rdf:about="&isdc;ISDC">
        <rdfs:label xml:lang="en">ISDC</rdfs:label>
        <rdfs:comment xml:lang="en">The ISDC ontology models main parts of the geoscience data and information life cycle. Related use cases are the representation of the ISDC data and information repsository as well as cross-domain data integration and retrieval.</rdfs:comment>
    </Class>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#Institution -->

    <Class rdf:about="&isdc;Institution">
        <rdfs:label xml:lang="en">Institution</rdfs:label>
        <rdfs:comment xml:lang="en">Institution is an entity belonging to the geoscience data and information life cycle. Institutions are research centers, universities, ...</rdfs:comment>
        <equivalentClass rdf:resource="&foaf;Organization"/>
        <rdfs:subClassOf rdf:resource="&isdc;ISDC"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&isdc;hasLocationKeyword"/>
                <someValuesFrom rdf:resource="&skos;Concept"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&isdc;supervises"/>
                <someValuesFrom rdf:resource="&isdc;Project"/>
            </Restriction>
        </rdfs:subClassOf>
        <disjointWith rdf:resource="&isdc;DataProduct"/>
        <disjointWith rdf:resource="&isdc;Instrument"/>
        <disjointWith rdf:resource="&isdc;Person"/>
        <disjointWith rdf:resource="&isdc;Phenomenon"/>
        <disjointWith rdf:resource="&isdc;Platform"/>
        <disjointWith rdf:resource="&isdc;ProductType"/>
        <disjointWith rdf:resource="&isdc;Project"/>
        <disjointWith rdf:resource="&isdc;Publication"/>
    </Class>
    


