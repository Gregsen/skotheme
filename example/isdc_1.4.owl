]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:gcmd="http://isdc.gfz-potsdam.de/ontology/gcmd.owl#"
     xmlns:isdc="http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#"
     xmlns:bibo="http://purl.org/ontology/bibo/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:gnd="http://d-nb.info/standards/elementset/gnd#">
    <Ontology rdf:about="http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl">
        <terms:hasVersion>1.4</terms:hasVersion>
        <terms:modified>2012-09-12</terms:modified>
        <terms:title xml:lang="en">ISDC Ontology</terms:title>
        <terms:description>ISDC ontology as information model for data lifecycle management for geosciences</terms:description>
        <terms:creator rdf:resource="http://isdc.gfz-potsdam.de/resource/2500043"/>
        <terms:contributor rdf:resource="http://isdc.gfz-potsdam.de/resource/2500066"/>
        <imports rdf:resource="http://isdc.gfz-potsdam.de/ontology/gcmd.owl"/>
        <imports rdf:resource="http://isdc.gfz-potsdam.de/ontology/gcmd_location.skos.rdf"/>
        <!--imports rdf:resource="http://isdc.gfz-potsdam.de/ontology/gcmd_science.skos.rdf"/>
        <imports rdf:resource="http://isdc.gfz-potsdam.de/ontology/gcmd_service.skos.rdf"/>
        <imports rdf:resource="http://isdc.gfz-potsdam.de/ontology/gemet-schema.rdf"/>
        <imports rdf:resource="http://isdc.gfz-potsdam.de/ontology/isdc_free_keywords.skos.rdf"/>
        <imports rdf:resource="http://isdc.gfz-potsdam.de/ontology/wgs84_pos.owl"/>
        <imports rdf:resource="http://purl.org/ontology/bibo/"/>
        <imports rdf:resource="http://xmlns.com/foaf/0.1/"/-->
    </Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <AnnotationProperty rdf:about="&terms;modified"/>
    <AnnotationProperty rdf:about="&skos;prefLabel">
        <rdfs:subPropertyOf rdf:resource="&rdfs;label"/>
    </AnnotationProperty>
    <AnnotationProperty rdf:about="&terms;description"/>
    <AnnotationProperty rdf:about="&terms;issued"/>
    <AnnotationProperty rdf:about="&terms;hasVersion"/>
    <AnnotationProperty rdf:about="&terms;publisher"/>
    



    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#carries -->

    <ObjectProperty rdf:about="&isdc;carries">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">carries</rdfs:label>
        <rdfs:comment xml:lang="en">A platform carries an instrument.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;Instrument"/>
        <rdfs:domain rdf:resource="&isdc;Platform"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;supplies"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#creates -->

    <ObjectProperty rdf:about="&isdc;creates">
        <rdfs:label xml:lang="en">creates</rdfs:label>
        <rdfs:comment xml:lang="en">An institution creates a product type.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:range rdf:resource="&isdc;ProductType"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#defines -->

    <ObjectProperty rdf:about="&isdc;defines">
        <rdfs:label xml:lang="en">defines</rdfs:label>
        <rdfs:comment xml:lang="en">A project defines a product type.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;ProductType"/>
        <rdfs:domain rdf:resource="&isdc;Project"/>
    </ObjectProperty>
    
    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#employs -->

    <ObjectProperty rdf:about="&isdc;employs">
        <rdfs:label xml:lang="en">employs</rdfs:label>
        <rdfs:comment xml:lang="en">An Institution employs a person.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:range rdf:resource="&isdc;Person"/>
        <inverseOf rdf:resource="&isdc;isEmployedBy"/>
    </ObjectProperty>


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#explains -->

    <ObjectProperty rdf:about="&isdc;explains">
        <rdfs:label xml:lang="en">explains</rdfs:label>
        <rdfs:comment xml:lang="en">Product types and data products explain geo phenomena.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;DataProduct"/>
        <rdfs:range rdf:resource="&isdc;GeoPhenomenon"/>
        <rdfs:domain rdf:resource="&isdc;ProductType"/>
        <inverseOf rdf:resource="&isdc;isExplainedBy"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasAuthor -->

    <ObjectProperty rdf:about="&isdc;hasAuthor">
        <rdfs:label xml:lang="en">has author</rdfs:label>
        <rdfs:comment xml:lang="en">A paper has an author.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;Author"/>
        <rdfs:domain rdf:resource="&isdc;Paper"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasDataCenterStaff -->

    <ObjectProperty rdf:about="&isdc;hasDataCenterStaff">
        <rdfs:label xml:lang="en">has data center staff</rdfs:label>
        <rdfs:comment xml:lang="en">Something can have data center staff.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;DataProvider"/>
        <rdfs:domain rdf:resource="&isdc;ISDC"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasDataCreator -->

    <ObjectProperty rdf:about="&isdc;hasDataCreator">
        <rdfs:label xml:lang="en">has data creator</rdfs:label>
        <rdfs:comment xml:lang="en">A data product can have a data creator.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;DataCreator"/>
        <rdfs:domain rdf:resource="&isdc;DataProduct"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasDataOriginator -->

    <ObjectProperty rdf:about="&isdc;hasDataOriginator">
        <rdfs:label xml:lang="en">has data originator</rdfs:label>
        <rdfs:comment xml:lang="en">A data product or product type can have a data originator.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;DataOriginator"/>
        <rdfs:domain rdf:resource="&isdc;DataProduct"/>
        <rdfs:domain rdf:resource="&isdc;ProductType"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
        <inverseOf rdf:resource="&isdc;isDataOriginatorOf"/>
        <equivalentProperty rdf:resource="&gcmd;Originating_Center"/>
    </ObjectProperty>
    



    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasDataProvider -->

    <ObjectProperty rdf:about="&isdc;hasDataProvider">
        <rdfs:label xml:lang="en">has data center</rdfs:label>
        <rdfs:comment xml:lang="en">Product types and/or data products are managed by an institution acting as data provider.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;DataProduct"/>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:range rdf:resource="&isdc;ProductType"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
        <inverseOf rdf:resource="&isdc;isDataProviderOf"/>
	<!-- equivalentProperty rdf:resource="&gcmd;Data_Center"/-->
    </ObjectProperty>
   



    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasFreeKeyword -->

    <ObjectProperty rdf:about="&isdc;hasFreeKeyword">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">has free keyword</rdfs:label>
        <rdfs:comment xml:lang="en">An entity has a free keyword. The proper use of the keyword is defined via concept schemes.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;ISDC"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasKeyword"/>
        <inverseOf rdf:resource="&isdc;isFreeKeywordFor"/>
        <equivalentProperty rdf:resource="&gcmd;Keyword"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasGeoLocation -->

    <ObjectProperty rdf:about="&isdc;hasGeoLocation">
        <rdfs:label xml:lang="en">has geo location</rdfs:label>
        <rdfs:comment xml:lang="en">Geo location connects an entity with a geonames location (URI). </rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:domain rdf:resource="&isdc;Instrument"/>
        <rdfs:domain rdf:resource="&isdc;Platform"/>
        <inverseOf rdf:resource="&isdc;isGeoLocationFor"/>
    </ObjectProperty>


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasHomepage -->

    <ObjectProperty rdf:about="&isdc;hasHomepage">
        <rdfs:label xml:lang="en">has homepage</rdfs:label>
        <rdfs:comment xml:lang="en">An institution/person/project has a homepage</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:domain rdf:resource="&isdc;Person"/>
        <rdfs:domain rdf:resource="&isdc;Project"/>
        <inverseOf rdf:resource="&isdc;isHomepageOf"/>
        <equivalentProperty rdf:resource="&foaf;homepage"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasIDNNode -->

    <ObjectProperty rdf:about="&isdc;hasIDNNode">
        <rdfs:label xml:lang="en">has IDN node</rdfs:label>
        <rdfs:comment xml:lang="en">An institution and a product type (GCMD DIF) have an IDN node.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:domain rdf:resource="&isdc;ProductType"/>
        <inverseOf rdf:resource="&isdc;isIDNNodeOf"/>
        <equivalentProperty rdf:resource="&gcmd;IDN_Node"/>
    </ObjectProperty>
    



    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasISOTopicCategory -->

    <ObjectProperty rdf:about="&isdc;hasISOTopicCategory">
        <rdfs:label xml:lang="en">has ISO topic category</rdfs:label>
        <rdfs:comment xml:lang="en">An entity has a ISO topic category keyword. The proper use of the keyword is defined via concept schemes.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasKeyword"/>
        <inverseOf rdf:resource="&isdc;isISOTopicCategoryFor"/>
        <equivalentProperty rdf:resource="&gcmd;ISO_Topic_Category"/>
    </ObjectProperty>



    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasInvestigator -->

    <ObjectProperty rdf:about="&isdc;hasInvestigator">
        <rdfs:label xml:lang="en">has investigator</rdfs:label>
        <rdfs:comment xml:lang="en">A project can have an investigator.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;Investigator"/>
        <rdfs:domain rdf:resource="&isdc;Project"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasKeyword -->

    <ObjectProperty rdf:about="&isdc;hasKeyword">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">has keyword</rdfs:label>
        <rdfs:comment xml:lang="en">An entity has a keyword. The proper use of the keyword is defined via concept schemes.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;ISDC"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasLanguage -->

    <ObjectProperty rdf:about="&isdc;hasLanguage">
        <rdfs:label xml:lang="en">has language</rdfs:label>
        <rdfs:comment xml:lang="en">Language of text in an entity.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;ISDC"/>
        <inverseOf rdf:resource="&isdc;isLanguageFor"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
	<equivalentProperty rdf:resource="&gcmd;Data_Set_Language"/>
    </ObjectProperty>



    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasLocationKeyword -->

    <ObjectProperty rdf:about="&isdc;hasLocationKeyword">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">has location keyword</rdfs:label>
        <rdfs:comment xml:lang="en">An entity has a location keyword. The proper use of the keyword is defined via concept schemes.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;DataProduct"/>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:domain rdf:resource="&isdc;Instrument"/>
        <rdfs:domain rdf:resource="&isdc;Platform"/>
        <rdfs:domain rdf:resource="&isdc;ProductType"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasKeyword"/>
        <equivalentProperty rdf:resource="&isdc;isLocatedIn"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasMetadataCreator -->

    <ObjectProperty rdf:about="&isdc;hasMetadataCreator">
        <rdfs:label xml:lang="en">has metadata creator</rdfs:label>
        <rdfs:comment xml:lang="en">A metadata publication can have a metadata creator.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;MetadataCreator"/>
        <rdfs:domain rdf:resource="&isdc;MetadataPublication"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasPhone -->

    <ObjectProperty rdf:about="&isdc;hasPhone">
        <rdfs:label xml:lang="en">has phone number</rdfs:label>
        <rdfs:comment xml:lang="en">Phone number of a person or institution.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Institution"/>
        <rdfs:domain rdf:resource="&isdc;Person"/>
        <inverseOf rdf:resource="&isdc;isPhoneNumberOf"/>
        <equivalentProperty rdf:resource="&foaf;phone"/>
        <equivalentProperty rdf:resource="&gcmd;Phone"/>
    </ObjectProperty>

   


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasProductType -->

    <ObjectProperty rdf:about="&isdc;hasProductType">
        <rdfs:label xml:lang="en">has product type</rdfs:label>
        <rdfs:comment xml:lang="en">A data product has a product type.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;DataProduct"/>
        <rdfs:range rdf:resource="&isdc;ProductType"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <equivalentProperty rdf:resource="&gcmd;Parent_DIF"/>
    </ObjectProperty>



    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasReleasePlace -->

    <ObjectProperty rdf:about="&isdc;hasReleasePlace">
        <rdfs:label xml:lang="en">has release place</rdfs:label>
        <rdfs:comment xml:lang="en">The release place of a dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;DataProduct"/>
        <rdfs:domain rdf:resource="&isdc;ProductType"/>
        <inverseOf rdf:resource="&isdc;isReleasePlaceOf"/>
        <equivalentProperty rdf:resource="&gcmd;Dataset_Release_Place"/>
    </ObjectProperty>
    



    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasResearchFocus -->

    <ObjectProperty rdf:about="&isdc;hasResearchFocus">
        <rdfs:label xml:lang="en">has research focus</rdfs:label>
        <rdfs:comment xml:lang="en">An entity has a research focus (keyword). The proper use of the keyword is defined via concept schemes.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasKeyword"/>
        <inverseOf rdf:resource="&isdc;isResearchFocusOf"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasRole -->

    <ObjectProperty rdf:about="&isdc;hasRole">
        <rdfs:label xml:lang="en">has role</rdfs:label>
        <rdfs:comment xml:lang="en">An institution or a person provide a role.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;ISDC"/>
        <rdfs:range rdf:resource="&isdc;Institution"/>
        <rdfs:range rdf:resource="&isdc;Person"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasScienceKeyword -->

    <ObjectProperty rdf:about="&isdc;hasScienceKeyword">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">has science keyword</rdfs:label>
        <rdfs:comment xml:lang="en">An entity has a science keyword. The proper use of the keyword is defined via concept schemes.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;ISDC"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasKeyword"/>
        <equivalentProperty rdf:resource="&gcmd;Parameters"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasServiceKeyword -->

    <ObjectProperty rdf:about="&isdc;hasServiceKeyword">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">has service keyword</rdfs:label>
        <rdfs:comment xml:lang="en">An entity has a service keyword. The proper use of the keyword is defined via concept schemes.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasKeyword"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasStatus -->

    <ObjectProperty rdf:about="&isdc;hasStatus">
        <rdfs:label xml:lang="en">status</rdfs:label>
        <rdfs:comment xml:lang="en">The status of the data set regarding its completeness (planned, in work or complete).</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;ProductType"/>
        <inverseOf rdf:resource="&isdc;isStatusOf"/>
        <equivalentProperty rdf:resource="&gcmd;Dataset_Progress"/>
    </ObjectProperty>
    
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#hasTechnicalStaff -->

    <ObjectProperty rdf:about="&isdc;hasTechnicalStaff">
        <rdfs:label xml:lang="en">has technical staff</rdfs:label>
        <rdfs:comment xml:lang="en">Something can have technical staff.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;ISDC"/>
        <rdfs:range rdf:resource="&isdc;TechnicalStaff"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;hasRole"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#isAuthorOf -->

    <ObjectProperty rdf:about="&isdc;isAuthorOf">
        <rdfs:label xml:lang="en">is author of</rdfs:label>
        <rdfs:comment xml:lang="en">A person is author of a scientific or technical publication.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Author"/>
        <rdfs:range rdf:resource="&isdc;Paper"/>
        <inverseOf rdf:resource="&isdc;hasAuthor"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;isInRoleOf"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#isCarriedBy -->

    <ObjectProperty rdf:about="&isdc;isCarriedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">is carried by</rdfs:label>
        <rdfs:comment xml:lang="en">An instrument is carried by a platform.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;Instrument"/>
        <rdfs:range rdf:resource="&isdc;Platform"/>
        <inverseOf rdf:resource="&isdc;carries"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;isPartOf"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#isCreatedBy -->

    <ObjectProperty rdf:about="&isdc;isCreatedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label xml:lang="en">is created by</rdfs:label>
        <rdfs:comment xml:lang="en">A product type is created by an institution.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;Institution"/>
        <rdfs:domain rdf:resource="&isdc;ProductType"/>
        <inverseOf rdf:resource="&isdc;creates"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#isDataCenterStaffOf -->

    <ObjectProperty rdf:about="&isdc;isDataCenterStaffOf">
        <rdfs:label xml:lang="en">is data center staff of</rdfs:label>
        <rdfs:comment xml:lang="en">A person is data staff of a data center.</rdfs:comment>
        <rdfs:domain rdf:resource="&isdc;DataProvider"/>
        <rdfs:range rdf:resource="&isdc;ISDC"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;isInRoleOf"/>
        <inverseOf rdf:resource="&isdc;hasDataCenterStaff"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#isDataCreatorOf -->

    <ObjectProperty rdf:about="&isdc;isDataCreatorOf">
        <rdfs:label xml:lang="en">is data creator of</rdfs:label>
        <rdfs:comment xml:lang="en">A person is data creator of a data product.</rdfs:comment>
        <rdfs:range rdf:resource="&isdc;DataProduct"/>
        <rdfs:domain rdf:resource="&isdc;Person"/>
        <rdfs:subPropertyOf rdf:resource="&isdc;isInRoleOf"/>
        <inverseOf rdf:resource="&isdc;hasDataCreator"/>
    </ObjectProperty>
    


    <!-- http://isdc.gfz-potsdam.de/ontology/isdc_1.4.owl#isDataOriginatorOf -->

